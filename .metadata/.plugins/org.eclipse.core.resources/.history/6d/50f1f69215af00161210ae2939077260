package ds;

public class BST {
	public Tnode root;
	BST()
	{
		root = null;
	}
	public Tnode  insert(Tnode root, int v)
	{
		if(root == null)
			root = new Tnode(v);
		if( v < root.data)
			root.left = insert(root.left,v);
		else if(v > root.data)
			root.right = insert(root.right,v);
		return root;
		
	}
}
