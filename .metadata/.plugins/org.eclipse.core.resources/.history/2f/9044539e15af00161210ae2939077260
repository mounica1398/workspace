package ds;
import java.util.Scanner;
public class Driver {
	private static Scanner s;

	public static void main(String [] args)
	{
		s = new Scanner(System.in);
		BST b = new BST();
		char ch;
		do
		{
			b.root = b.insert(b.root, s.nextInt());
			System.out.println("do you want to continue ? y / n ");
			ch = s.next().charAt(0);
		}while(ch == 'y');
		
		
		
	}

}
